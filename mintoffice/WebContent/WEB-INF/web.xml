<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>mintoffice</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>DispatcherServlet</display-name>
    <servlet-name>DispatcherServlet</servlet-name>
    <servlet-class>control.DispatcherServlet</servlet-class>
    <init-param>
      <param-name>contextConfigLocation</param-name>
      <param-value>/WEB-INF/mintoffice.properties</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>DispatcherServlet</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>CharacterEncodingFilter</display-name>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
    <dispatcher>REQUEST</dispatcher>
  </filter-mapping>
  <listener>
    <listener-class>listener.ContextLoaderListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>EachController</display-name>
    <servlet-name>EachController</servlet-name>
    <servlet-class>control.personal.EachController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EachController</servlet-name>
    <url-pattern>/EachController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EmpInfoServlet</display-name>
    <servlet-name>EmpInfoServlet</servlet-name>
    <servlet-class>control.personal.EmpInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmpInfoServlet</servlet-name>
    <url-pattern>/empinfo.bo</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FileListServlet</display-name>
    <servlet-name>FileListServlet</servlet-name>
    <servlet-class>control.business.FileListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileListServlet</servlet-name>
    <url-pattern>/filelist.bo</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FindZipServlet</display-name>
    <servlet-name>FindZipServlet</servlet-name>
    <servlet-class>control.personal.FindZipServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FindZipServlet</servlet-name>
    <url-pattern>/findzip.bo</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FileWriteServlet</display-name>
    <servlet-name>FileWriteServlet</servlet-name>
    <servlet-class>control.business.FileWriteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileWriteServlet</servlet-name>
    <url-pattern>/filewrite.bo</url-pattern>
  </servlet-mapping>  
</web-app>